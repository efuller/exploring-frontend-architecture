{
  "name": "exploring-frontend-architecture",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview --host=0.0.0.0 --port=10000",
    "staging:test:e2e": "NODE_ENV=staging jest --selectProjects e2e",
    "test": "jest",
    "test:app": "jest --selectProjects app",
    "test:app:dev": "jest --selectProjects app --watchAll",
    "test:dev": "jest --watchAll",
    "test:e2e": "jest --selectProjects e2e",
    "test:e2e:dev": "jest --selectProjects e2e --watchAll",
    "test:infra": "jest --selectProjects infra",
    "test:infra:dev": "jest --selectProjects infra --watchAll",
    "tsc": "tsc --noEmit"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.15",
    "@heroicons/react": "^2.0.18",
    "axios": "^1.4.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.45.0",
    "uuid": "^9.0.0",
    "vite": "^4.3.9"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.3",
    "@types/jest": "^29.5.5",
    "@types/react": "^18.0.37",
    "@types/react-dom": "^18.0.11",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "@vitejs/plugin-react-swc": "^3.0.0",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.38.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.3.4",
    "jest": "^29.7.0",
    "jest-cucumber": "^3.0.1",
    "jest-environment-jsdom": "^29.7.0",
    "jest-puppeteer": "^9.0.0",
    "postcss": "^8.4.24",
    "puppeteer": "^21.3.1",
    "tailwindcss": "^3.3.2",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.2"
  }
}
