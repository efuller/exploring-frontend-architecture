{
  "name": "exploring-frontend-architecture",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview --host=0.0.0.0 --port=10000",
    "staging:test:e2e": "NODE_ENV=staging jest --selectProjects e2e",
    "test": "jest",
    "test:app": "jest --selectProjects app",
    "test:app:dev": "jest --selectProjects app --watchAll",
    "test:dev": "jest --watchAll",
    "test:e2e": "jest --selectProjects e2e",
    "test:e2e:dev": "jest --selectProjects e2e --watchAll",
    "test:infra": "jest --selectProjects infra",
    "test:infra:dev": "jest --selectProjects infra --watchAll",
    "tsc": "tsc --noEmit"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.0",
    "@heroicons/react": "^2.2.0",
    "@tailwindcss/postcss": "^4.0.8",
    "axios": "^1.7.9",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.54.2",
    "uuid": "^11.1.0",
    "vite": "^6.1.1"
  },
  "devDependencies": {
    "@eslint/compat": "^1.2.7",
    "@eslint/eslintrc": "^3.3.0",
    "@eslint/js": "^9.21.0",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/vite": "^4.0.8",
    "@types/jest": "^29.5.14",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.24.1",
    "@typescript-eslint/parser": "^8.24.1",
    "@vitejs/plugin-react-swc": "^3.8.0",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.21.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "jest": "^29.7.0",
    "jest-cucumber": "^4.5.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-puppeteer": "^11.0.0",
    "postcss": "^8.5.3",
    "puppeteer": "^24.2.1",
    "tailwindcss": "^4.0.8",
    "ts-jest": "^29.2.6",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.3"
  }
}
